    <param pos="0" name="os.vendor" value="CentOS"/>
    <param pos="0" name="os.family" value="Linux"/>
    <param pos="0" name="os.product" value="Linux"/>
    <param pos="1" name="os.version"/>
  </fingerprint>
  <fingerprint pattern="^(?i:Debian(?: Linux)?\s?(\d+?(?:\.\d+?)+?|\d+?)?(?:\s[a-z\(\)]+)?)$">
