  <!-- Linux catch-all goes at the bottom-->
  <fingerprint pattern="^(?i:.*Linux?\s?(\d+?(?:\.\d+?)*?)?)$">
    <description>Linux catch-all</description>
    <example os.version="2.42.6">Linux 2.42.6</example>
